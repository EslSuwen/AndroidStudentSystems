<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cqju.studentsystem">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:networkSecurityConfig="@xml/network_config">
        <activity android:name="com.cqju.com.cqjtu.studentsystem.RegisterActivity"></activity>
        <activity android:name="com.cqju.com.cqjtu.studentsystem.LoginActivity" />
        <activity android:name="com.cqju.com.cqjtu.studentsystem.StudentActivity" />
        <activity android:name="com.cqju.com.cqjtu.studentsystem.AdminActivity" />
        <activity android:name="com.cqju.com.cqjtu.studentsystem.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>